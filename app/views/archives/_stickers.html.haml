- objects.each do |object|
  .col-xs-6
    .panel.panel-primary
      .panel-heading
        .row
          .col-xs-6.panel-title
          .col-xs-offset-2.col-xs-4.panel-title
            = link_to restore_object_path(object_name: object.class.to_s, id: object), method: :delete, class: 'glyph_destroy_link' do
              %i.glyphicon.glyphicon-open
      .panel-body
        .row
          .col-xs-12.sticker-desc
            = object.description
