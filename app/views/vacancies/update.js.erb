$('#dialog').modal('toggle');
$('.vacancy_status').html('<%= escape_javascript(render partial: 'vacancy_status',
                                                        locals: { vacancy: @vacancy }) %>');
$('.vacancy-candidates').html('<%= raw escape_javascript(render partial: 'vacancy_candidates',
                                                                locals: { vacancy_candidates: @vacancy_candidates,
                                                                          vacancy: @vacancy,
                                                                          status: @status }) %>');
$('.modal-backdrop').remove();
